                                <span class="LoginText">未登入</span>
                            </a>
                      </li>
                  </ul>
              </div>
              <!-- End Atribute Navigation -->
              <!--會員登入互動視窗Start-->
                <div class="modal fade" id="loginModal" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1"
                    aria-labelledby="loginModalLabel" aria-hidden="true">
                    <div class="modal-dialog modal-dialog-centered">
                        <div class="modal-content">
                            <div class="d-flex align-items-center modal-header">
                                <h5 class=" modal-title" id="loginModalLabel">登入</h5>
                                <button type="button" class="btn-close" data-bs-dismiss="modal"
                                    aria-label="Close"></button>
                            </div>
                            <div class="modal-body">
                                <div class="mt-2 icon d-flex align-items-center justify-content-center">
                                    <img src="~/images/CatCha/profile_Icon.png">
                                </div>
                                <div class="row justify-content-md-center">
                                    <div class="col-10">
                                        <div class="container-fluid">
                                            <form class="needs-validation" novalidate>
                                                <div class="row">
                                                    <div>
                                                        <label for="login-name">電子信箱：</label>
                                                        <input type="text" class="form-control" id="login-name"
                                                            placeholder="email" required>
                                                        <div class="invalid-feedback"> 請輸入電子信箱</div>
                                                    </div>
                                                    <div>
                                                        <label for="login-password">密碼：</label>
                                                        <input type="text" class="form-control" id="login-password"
                                                            placeholder="password" required>
                                                        <div class="invalid-feedback"> 請輸入密碼 </div>
                                                    </div>
                                                </div>

                                                <div class="d-grid gap-2 mt-3">
                                                    <button class="btn btn-primary" type="button">登入</button>
                                                </div>
                                                <div>
                                                    <div
                                                        class="d-flex justify-content-between align-items-center mb-3 mt-2">
                                                        <div class="d-flex justify-content-between">
                                                            <div class="form-check form-switch">
                                                                <input class="form-check-input" type="checkbox"
                                                                    @* role="switch"  *@id="loginCheckDefault">
                                                                <label class="form-check-label"
                                                                    for="loginCheckDefault">記住我</label>
                                                            </div>
                                                        </div>
                                                        <div class=" d-flex justify-content-end">
                                                            <a href="#" class="link-color link-primary"
                                                                data-bs-toggle="modal"
                                                                data-bs-target="#forgotPasswordModal">忘記密碼?</a>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="modal-footer row gy-4" ></div>
                                                <div class="d-grid gap-2">
                                                    <button class="btn btn-light" type="button">加入會員</button>
                                                </div>
                                                <div class="mt-2 text-center">
                                                    <a>或用其他社群登入</a>
                                                </div>
                                                <div class="mt-3 text-center">
                                                    <a href="#">
                                                        <span><img src="images/CatCha/fb_icon.png"></span>
                                                    </a>
                                                    <a href="#">
                                                        <span><img src="images/CatCha/google_icon.png"></span>
                                                    </a>
                                                    <a href="#">
                                                        <span> <img src="images/CatCha/twitter_icon.png"></span>
                                                    </a>
                                                </div>
                                                </form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
