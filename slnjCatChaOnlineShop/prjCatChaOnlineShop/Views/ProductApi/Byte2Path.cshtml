
@{
    ViewData["Title"] = "Byte2Path";
}

<h1>Byte2Path</h1>
@using (Html.BeginForm())
{

    <hr />    
    <div class="form-group">
        <div class="col-md-offset-0 col-md-10">
            商品ID
        </div>
        <div class="col-md-10">
            @Html.TextBox("txtPId", "", new { htmlAttributes = new { @class = "form-control" } })

        </div>
        <div class="col-md-offset-0 col-md-10">
            商品圖序號
        </div>
        <div class="col-md-10">
            @Html.TextBox("txtNum", "", new { htmlAttributes = new { @class = "form-control" } })
        </div>
        
    </div>

    <div class="form-group">
        <div class="col-md-offset-0 col-md-10">
            <input type="submit" value="更改" class="btn btn-default" />
        </div>
    </div>



    
}


